{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "beta",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-apcu": "^5.1",
        "ext-ctype": "*",
        "ext-gmp": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-sodium": "*",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "nelmio/cors-bundle": "^2.3",
        "nelmio/security-bundle": "^3.0",
        "nyholm/psr7": "^1.5",
        "runtime/frankenphp-symfony": "^0.1.1",
        "symfony/asset": "^6.3",
        "symfony/asset-mapper": "^6.3",
        "symfony/console": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/flex": "^1.17|^2",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/process": "6.4.*",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/proxy-manager-bridge": "^6.3",
        "symfony/runtime": "^6.4",
        "symfony/security-bundle": "6.4.*",
        "symfony/security-csrf": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/stimulus-bundle": "^2.12",
        "symfony/string": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/uid": "6.4.*",
        "symfony/ux-typed": "^2.12",
        "symfony/validator": "6.4.*",
        "symfony/yaml": "6.4.*",
        "symfonycasts/tailwind-bundle": "^0.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.3",
        "twig/twig": "^2.12|^3.0",
        "web-auth/metadata-service": "^4.1.2",
        "web-auth/webauthn-lib": "^4.1.2",
        "web-auth/webauthn-stimulus": "^4.1.2",
        "web-auth/webauthn-symfony-bundle": "^4.1.2",
        "web-token/jwt-key-mgmt": "^3.0",
        "web-token/jwt-signature-algorithm-ecdsa": "^3.0",
        "web-token/jwt-signature-algorithm-experimental": "^3.0",
        "web-token/jwt-signature-algorithm-rsa": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "compile": [
            "php -d memory_limit=512M bin/console doctrine:migrations:migrate --no-interaction",
            "php bin/console importmap:update",
            "php bin/console tailwind:build --minify",
            "php bin/console asset-map:compile"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*",
            "endpoint": ["https://api.github.com/repos/web-auth/recipes/contents/index.json?ref=main", "flex://defaults"]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "rector/rector": "^0.18",
        "roave/security-advisories": "dev-latest",
        "symfony/debug-bundle": "6.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "6.4.*",
        "symfony/stopwatch": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*",
        "symplify/easy-coding-standard": "^12.0"
    }
}
